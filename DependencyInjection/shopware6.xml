            <tag name="shopware.migration.reader"/>
        </service>

        <service id="SwagMigrationAssistant\Profile\Shopware6\Gateway\Api\Reader\TaxReader"
                 parent="SwagMigrationAssistant\Profile\Shopware6\Gateway\Api\Reader\ApiReader">
            <tag name="shopware.migration.reader"/>
